{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2022",
    "moduleResolution": "Node",
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "jsx": "preserve",
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "types": [ "node", "vite/client"],
    "typeRoots": ["."],
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
